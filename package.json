{
  "name": "next-beer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html}\" --ignore-path .gitignore",
    "lint:style:fix": "stylelint --fix \"**/*.{css,scss,sass,html}\" --ignore-path .gitignore",
